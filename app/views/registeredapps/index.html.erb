<h1>My Registered Applications</h1>
                <div class="row">
                  <div class="col-md-8">
                    <% @registeredapps.each do |registeredapp| %>
                      <div class="media">
                        <div class="media-body">
                          <h4 class="media-heading">
                            <%= link_to registeredapp.name, registeredapps_path %>
                          </h4>
                            <%= registeredapp.url %>
                            <%= link_to 'Show', registeredapp %>
                            <%= link_to 'Edit', edit_registeredapp_path(registeredapp) %>
                        </div>
                      </div>
                    <% end %>
                  </div>
                </div>
                <%= link_to "New Application", new_registeredapp_path, class: 'btn btn-success' %>
  </div>
</div>
